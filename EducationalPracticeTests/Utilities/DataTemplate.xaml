<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:EducationalPracticeTests.ViewModel"
                    xmlns:view="clr-namespace:EducationalPracticeTests.View">
    <DataTemplate DataType="{x:Type vm:UserVM}">
        <view:User/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TestVM}">
        <view:Test/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ResultTestVM}">
        <view:ResultTest/>
    </DataTemplate>

  
</ResourceDictionary>